@model ConsumeGroceryMartAPI.Models.User



@{
    ViewBag.Title = "Grocery Mart";
}

<body>
    <div class="well">        
            <div class="col-lg-12 row">
                <div class="col-lg-5">

                    @{Html.RenderPartial("RegisterUser"); }
                   
                    <div>
                        @*<p class="text-danger"> @ViewBag.errorMsgForLogin</p>*@
                        <p class="text-success">@TempData["SuccessMsg"]</p>
                        <p class="text-danger"> @TempData["errorForUser"]</p>
                    </div>
                </div>
                <div class="col-lg-5 pull-right">
                    @{Html.RenderPartial("SignUp");}
                    <div>
                        @*<p class="text-danger"> @ViewBag.errorMsgForLogin</p>*@
                        <p class="text-danger">@TempData["errorMsgForLogin"]</p>     
                    </div>
                </div>
            </div>
        
        @{ Html.RenderPartial("GmailLogin"); }

    </div>

</body>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
